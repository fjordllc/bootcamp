{
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended",
    "standard",
    "prettier"
  ],
  "env": {
    "jquery": true,
    "node": true
  },
  "globals": {
    "window": true,
    "fetch": true,
    "FileReader": true,
    "Event": true,
    "FormData": true
  },
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-unused-vars": ["error", {
      "args": "all",
      "argsIgnorePattern": "^_"
    }]
  }
}
