- title '提出物作成'
- set_meta_tags description: '提出物作成ページです。'

header.page-header
  .container
    .page-header__inner
      h2.page-header__title
        = title
hr.a-border
.page-body
  .page-body__rows
    .page-body__row
      .container.is-lg
        = render 'product_practice_content', product: @product, is_description: true
        = render 'product_practice_content', product: @product, is_description: false
    .page-body__row
      .container.is-xl
        = render 'form', practice: @practice, product: @product
