doctype html
html
  head
    - page_title = yield :title
    title #{page_title.present? ? page_title + ' - ' : ''}#{t('256interns')}
    = stylesheet_link_tag    'admin', media: 'all'
    = javascript_include_tag 'admin'
    = csrf_meta_tags
  body class="#{controller_name}_#{action_name}"
    .container
      #header
        = render 'nav'
        h1= link_to t('256interns'), root_path
        = render 'alert'
      .row
        .span12= yield
      #footer
        | Â©
        = link_to 'FJORD, LLC', 'http://fjord.jp'
